{
    "name": "patriciamvc/api",
    "description": "High Speed Php Framework For Code Engineers",
    "type": "project",
    "version": "v0.9.9",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "firebase/php-jwt": "dev-master",
        "php-di/php-di": "^6.0.11",
        "sevens/router": "^1.0.0",
        "sevens/vars": "^1.0.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Providers\\": "app/Providers/",
            "App\\Http\\Controllers\\": "app/Http/Controllers/",
            "App\\Http\\Middlewares\\": "app/Http/Middlewares/"
        },
        "files": [
                "app/Helpers/Helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "@php Engineer app::start"
        ]
    },
    "keywords": [ "SevenPHP", "API", "framework", "engineer" ]
}
